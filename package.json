{
  "name": "bitcoin-practice",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://fsultani.github.io/bitcoin-practice",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "axios": "^0.20.0",
    "moment": "^2.27.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-grid-system": "^7.0.3",
    "react-lottie": "^1.2.3",
    "react-scripts": "^3.4.3",
    "styled-components": "^5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.4.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --single-quote --trailing-comma  --write",
      "eslint --fix",
      "git add"
    ],
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
